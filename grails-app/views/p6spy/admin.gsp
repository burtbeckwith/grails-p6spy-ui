<!doctype html>
<html>
<head>
<meta name="layout" content="main">
<title>P6Spy Info</title>
</head>

<body>

	<h3>P6Spy Configuration Information:</h3>

	<table>
		<thead>
			<tr><th>Name</th><th>Value</th></tr>
		</thead>
		<tbody>
			<tr><td>Outage Detection Enabled</td><td>${outageDetection}&nbsp;</td></tr>
			<tr><td>outageDetectionInterval</td><td>${outageDetectionInterval}&nbsp;</td></tr>
			<tr><td>outageDetectionIntervalMS</td><td>${outageDetectionIntervalMS}&nbsp;</td></tr>
			<tr><td>executionThreshold</td><td>${executionThreshold}&nbsp;</td></tr>
			<tr><td>usePrefix</td><td>${usePrefix}&nbsp;</td></tr>
			<tr><td>autoflush</td><td>${autoflush}&nbsp;</td></tr>
			<tr><td>exclude</td><td>${exclude}&nbsp;</td></tr>
			<tr><td>excludecategories</td><td>${excludecategories}&nbsp;</td></tr>
			<tr><td>filter</td><td>${filter}&nbsp;</td></tr>
			<tr><td>include</td><td>${include}&nbsp;</td></tr>
			<tr><td>includecategories</td><td>${includecategories}&nbsp;</td></tr>
			<tr><td>deregisterDrivers</td><td>${deregisterDrivers}&nbsp;</td></tr>
			<tr><td>logfile</td><td>${logfile}&nbsp;</td></tr>
			<tr><td>appender</td><td>${appender}&nbsp;</td></tr>
			<tr><td>Driver</td><td>${realdriver}&nbsp;</td></tr>
			<tr><td>Driver 2</td><td>${realdriver2}&nbsp;</td></tr>
			<tr><td>Driver 3</td><td>${realdriver3}&nbsp;</td></tr>
			<tr><td>append</td><td>${append}&nbsp;</td></tr>
			<tr><td>spydriver</td><td>${spydriver}&nbsp;</td></tr>
			<tr><td>dateformat</td><td>${dateformat}&nbsp;</td></tr>
			<tr><td>stringmatcher</td><td>${stringmatcher}&nbsp;</td></tr>
			<tr><td>Stack Trace Enabled</td><td>${stackTrace}&nbsp;</td></tr>
			<tr><td>stackTraceClass</td><td>${stackTraceClass}&nbsp;</td></tr>
			<tr><td>sqlExpression</td><td>${sqlExpression}&nbsp;</td></tr>
			<tr><td>Reload Properties Enabled</td><td>${reloadProperties}&nbsp;</td></tr>
			<tr><td>reloadPropertiesInterval</td><td>${reloadPropertiesInterval}&nbsp;</td></tr>
			<tr><td>jndiContextFactory</td><td>${jndiContextFactory}&nbsp;</td></tr>
			<tr><td>jndiContextProviderURL</td><td>${jndiContextProviderURL}&nbsp;</td></tr>
			<tr><td>jndiContextCustom</td><td>${jndiContextCustom}&nbsp;</td></tr>
			<tr><td>realDataSource</td><td>${realDataSource}&nbsp;</td></tr>
			<tr><td>realDataSourceClass</td><td>${realDataSourceClass}&nbsp;</td></tr>
			<tr><td>realDataSourceProperties</td><td>${realDataSourceProperties}&nbsp;</td></tr>
			<tr><td>allModules</td><td>${allModules.join(', ')}&nbsp;</td></tr>
			<tr><td>allDriverNames</td><td>${allDriverNames.join(', ')}&nbsp;</td></tr>
		</tbody>
	</table>

</body>
</html>
